{
  "name": "Tispyl_Backend",
  "version": "1.0.0",
  "description": "Backend for the famous TischSpiel",
  "main": "dist/app.js",
  "scripts": {
    "setup": "cd scripts && setup.sh",
    "start": "nodemon app.ts",
    "start:debugger": "node --inspect -r ts-node/register app.ts",
    "start:prod": "tsc && node dist/app.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "updateRemote": "deployment\\updateRemote.cmd deployment_rsa",
    "startRemote": "deployment\\startRemote.cmd deployment_rsa",
    "shutDownRemote": "deployment\\shutDownRemote.cmd deployment_rsa",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:show": "npm run typeorm migration:show",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,json,css}": [
      "eslint --fix",
      "git add -A"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@colyseus/monitor": "^0.14.22",
    "@types/express": "^4.17.8",
    "@types/multer": "^1.4.4",
    "@types/node": "^16.11.12",
    "atob": "^2.1.2",
    "better-logging": "^4.4.0",
    "cannon": "^0.6.2",
    "class-validator": "^0.13.1",
    "colyseus": "^0.14.23",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "jimp": "^0.10.3",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.5.1",
    "marked": "^4.0.19",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "node-mailjet": "^3.3.4",
    "object-hash": "^2.1.1",
    "serve-index": "^1.8.0",
    "superagent": "^3.8.1",
    "three": "^0.143.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/cannon": "^0.1.5",
    "@types/three": "^0.141.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-eslint": "^10.1.0",
    "browserify": "^14.4.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-wesbos": "^3.0.2",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^3.1.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.12",
    "prettier": "^2.2.1",
    "ts-node": "^8.10.2",
    "typeorm": "^0.2.31",
    "typescript": "^4.7.0"
  }
}
